@model IEnumerable<VillaBisutti.Delta.Core.Model.ItemBoloDoceBemCasadoSelecionado>
@{
	Layout = null;
}
@if (Model.Count() > 0)
{
	<table class="table">
		<tr>
			<th width="20%"><!--20-->
				@Html.DisplayNameFor(model => model.ItemBoloDoceBemCasado.Nome)
			</th>
			<th width="20%"><!--40-->
				@Html.DisplayNameFor(model => model.ItemBoloDoceBemCasado.Fornecedor)
			</th>
			<th width="10%"><!--50-->
				@Html.DisplayNameFor(model => model.Quantidade)
			</th>
			<th width="10%"><!--60-->
				@Html.DisplayNameFor(model => model.HorarioEntrega)
			</th>
			<th width="2%"><!--62-->
				@Html.DisplayNameFor(model => model.Definido)
			</th>
			<th width="2%"><!--64-->
				@Html.DisplayNameFor(model => model.FornecedorStartado)
			</th>
			<th width="2%"><!--66-->
				@Html.DisplayNameFor(model => model.Contratado)
			</th>
			<th width="32%"><!--98-->
				@Html.DisplayNameFor(model => model.Observacoes)
			</th>
			<th width="2%"></th><!--100-->
		</tr>
		@foreach (var item in Model)
		{
			@Html.Partial("../ItemBoloDoceBemCasadoSelecionado/EditFornecimentoTerceiro", item)
		}
	</table>
}
else
{
	<p>Não existem itens adicionados à OS com esse criterio</p>
}