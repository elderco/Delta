@model IEnumerable<VillaBisutti.Delta.Core.Model.ItemBebidaSelecionado>
@{
	Layout = null;
}
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.ContratoAditivo.Arquivo)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.ItemBebida.Nome)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.EventoId)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Definido)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Contratado)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.ContratacaoBisutti)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.FornecimentoBisutti)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.FornecedorStartado)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Quantidade)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.HorarioEntrega)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.ContatoFornecimento)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Observacoes)
		</th>
		<th></th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.ContratoAditivo.Arquivo)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.ItemBebida.Nome)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.EventoId)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Definido)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Contratado)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.ContratacaoBisutti)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.FornecimentoBisutti)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.FornecedorStartado)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Quantidade)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.HorarioEntrega)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.ContatoFornecimento)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Observacoes)
			</td>
			<td>
				@Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
				@Html.ActionLink("Details", "Details", new { id = item.Id }) |
				@Html.ActionLink("Delete", "Delete", new { id = item.Id })
			</td>
		</tr>
	}

</table>
