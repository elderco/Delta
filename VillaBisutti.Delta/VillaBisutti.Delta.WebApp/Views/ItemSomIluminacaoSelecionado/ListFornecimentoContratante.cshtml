@model IEnumerable<VillaBisutti.Delta.Core.Model.ItemSomIluminacaoSelecionado>
@{
    Layout = null;
}
@if (Model.Count() > 0)
{
    <table class="table">
        <tr>
            <th class="col-md-4">
                @Html.DisplayNameFor(model => model.ItemSomIluminacao.Nome)
            </th>
            <th class="col-md-1">
                @Html.DisplayNameFor(model => model.Quantidade)
            </th>
            <th class="col-md-2">
                @Html.DisplayNameFor(model => model.ContatoFornecimento)
            </th>
            <th class="col-md-1">
                @Html.DisplayNameFor(model => model.HorarioMontagem)
            </th>
            <th class="col-md-3">
                @Html.DisplayNameFor(model => model.Observacoes)
            </th>
            <th class="col-md-1"></th>
        </tr>
        <tbody>
            @foreach (var item in Model)
            {
                @Html.Action("EditFornecimentoContratante", "ItemSomIluminacaoSelecionado", new { id = item.Id })
            }
        </tbody>
    </table>
}
else
{
    <p>Não existem itens adicionados à OS com esse criterio</p>
}