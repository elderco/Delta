@model VillaBisutti.Delta.Core.Model.TipoPratoPadrao
@{
	Layout = null;
}
<script language="javascript" type="text/javascript">
	$(document).ready(function () {
		AddStack("Pratos do cardápio", "Quantidade de pratos definida para @(Model.TipoPrato.Nome): escolher @(Model.QuantidadePratos) pratos");
		$("#Quantidade_@(Model.Id)").text("@(Model.QuantidadePratos)");
	});
</script>
