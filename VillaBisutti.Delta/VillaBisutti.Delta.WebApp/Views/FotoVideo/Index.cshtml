@model VillaBisutti.Delta.Core.Model.FotoVideo
@{
	Layout = "~/Views/Shared/_EventosLayout.cshtml";
}
@section scripts {
	<script language="javascript" type="text/javascript">
		$(document).ready(function () {
			$(".FormItemEditor").submit(function (evt) {
				//evt.preventDefault();
				//alert($(this).attr("action"));
			});
		});
	</script>
}
<div class="row">
	<fieldset class="col-md-4">
		<legend>Informações básicas</legend>
		@Html.Action("Edit", "FotoVideo", new { id = @ViewBag.Id }) @*Beleza*@
	</fieldset>
	<fieldset class="col-md-7 pull-right">
		<legend>Adicionar item</legend>
		@Html.Action("Create", "ItemFotoVideoSelecionado", new { id = @ViewBag.Id })
	</fieldset>
</div>
<div class="row">
	@Html.Action("ListNaoSelecionados", "TipoItemFotoVideo", new { id = @ViewBag.Id }) @*Beleza*@
</div>
<div class="row">
	<h3>Itens Villa Bisutti (Contratação e Fornecimento)</h3>
	@Html.Action("ListFornecimentoBisutti", "ItemFotoVideoSelecionado", new { id = @ViewBag.Id })
</div>
<div class="row">
	<h3>Fornecedores externos (Contratação Villa Bisutti)</h3>
	@Html.Action("ListFornecimentoTerceiro", "ItemFotoVideoSelecionado", new { id = @ViewBag.Id })
</div>
<div class="row">
	<h3>Fornecedores externos (Responsabilidade do contratante)</h3>
	@Html.Action("ListFornecimentoContratante", "ItemFotoVideoSelecionado", new { id = @ViewBag.Id })
</div>
