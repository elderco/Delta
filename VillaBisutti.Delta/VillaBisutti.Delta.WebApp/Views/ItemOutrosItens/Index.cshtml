@model IEnumerable<VillaBisutti.Delta.Core.Model.ItemOutrosItens>
@{
	Layout = "~/Views/Shared/_ConfiguracoesLayout.cshtml";
	//ViewBag.Title = "Index";
}

@section scripts {

    <script language="javascript" type="text/javascript">
        $(document).ready(function () {
            $(".PopUpActionLinks").click(function (e) {
                e.preventDefault();
                ShowPopUp($(this).attr("href"), $(this).attr("title"), 790);
            });
        });
    </script>
}

@Html.Action("Create", "ItemOutrosItens")