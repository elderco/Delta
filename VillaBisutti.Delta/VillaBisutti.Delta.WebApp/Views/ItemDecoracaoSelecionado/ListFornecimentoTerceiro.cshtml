@model IEnumerable<VillaBisutti.Delta.Core.Model.ItemDecoracaoSelecionado>
@{
	Layout = null;
}
@if (Model.Count() > 0)
{
	<table class="table">
		<tr>
			<th class="col-md-2">
				@Html.DisplayNameFor(model => model.ItemDecoracao.Nome)
			</th>
			<th class="col-md-1">
				@Html.DisplayNameFor(model => model.Quantidade)
			</th>
			<th class="col-md-2">
				@Html.DisplayNameFor(model => model.ContatoFornecimento)
			</th>
			<th class="col-md-1">
				@Html.DisplayNameFor(model => model.HorarioMontagem)
			</th>
			<th class="col-md-1">
				@Html.DisplayNameFor(model => model.Definido)
			</th>
			<th class="col-md-1">
				@Html.DisplayNameFor(model => model.FornecedorStartado)
			</th>
			<th class="col-md-1">
				@Html.DisplayNameFor(model => model.Contratado)
			</th>
			<th class="col-md-2">
				@Html.DisplayNameFor(model => model.Observacoes)
			</th>
			<th class="col-md-1"></th>
		</tr>
		@foreach (var item in Model)
		{
			@Html.Partial("../ItemDecoracaoSelecionado/EditFornecimentoTerceiro", item)
		}
	</table>
}
else
{
	<p>Não existem itens adicionados à OS com esse criterio</p>
}